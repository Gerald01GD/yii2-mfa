{
    "name": "vxm/yii2-mfa",
    "description": "Multi-factor authentication for yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","mfa"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": ">=2.0.13",
        "spomky-labs/otphp": "^11.3.0",
        "endroid/qr-code": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "vxm\\mfa\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
